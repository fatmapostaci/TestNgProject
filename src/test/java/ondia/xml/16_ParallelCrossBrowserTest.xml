    <test verbose="2" preserve-order="true"
          name="FireFox CrossBrowser Test" >
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="ondia.tests.C22ParallelCrossBrowserTestThreadLocal"></class>

        </classes>
    </test>
</suite>